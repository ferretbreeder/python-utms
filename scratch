def mail_tel_img_link_remover(links):

    for link in links:
        if "mailto:" in link:
            del(link, links[i])
        elif "tel:" in link:
            links.del(link)
        elif ".jpg" in link:
            links.del(link)
        elif ".png" in link:
            links.del(link)
        else:
            pass

    return links

linksTest = ['https://admissions.indiana.edu/contact/counselors/index.html', 'tel:812-369-9157', 'https://cts.admissions.indiana.edu/', 'https://admissions.indiana.edu/apply/freshman/ap-exam-credit.html', "test.png", "https://admissions.indiana.edu/apply/freshman/ap-exam-credit.html.png", 'https://cts.admissions.indiana.edu/']
